{
  "name": "@1024pix/ember-testing-library",
  "version": "3.0.6",
  "description": "Package for Testing Library usage in Ember applications.",
  "keywords": [
    "ember-addon",
    "testing",
    "ui",
    "dom",
    "unit",
    "integration",
    "functional",
    "end-to-end",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1024pix/ember-testing-library.git"
  },
  "license": "MIT",
  "author": "Pix",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:watch": "npm-run-all lint test:*",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/test-helpers": "^4.0.0",
    "@testing-library/dom": "10.4.0",
    "broccoli-funnel": "^3.0.8",
    "ember-auto-import": "^2.6.0",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.11.0",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@ember/optional-features": "^2.1.0",
    "@embroider/test-setup": "^4.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^5.10.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.0",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^8.0.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "^5.0.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.0.0",
    "qunit": "^2.19.0",
    "qunit-dom": "^3.0.0",
    "testem": "^3.10.1",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "engines": {
    "node": "^20 || ^22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/1024pix/ember-testing-library/issues"
  },
  "homepage": "https://github.com/1024pix/ember-testing-library#readme",
  "main": "index.js"
}
