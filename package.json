{
  "name": "@1024pix/ember-testing-library",
  "version": "0.6.2",
  "description": "Package for Testing Library usage in Ember applications.",
  "keywords": [
    "ember-addon",
    "testing",
    "ui",
    "dom",
    "unit",
    "integration",
    "functional",
    "end-to-end",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1024pix/ember-testing-library.git"
  },
  "license": "MIT",
  "author": "Pix",
  "scripts": {
    "build": "npm run build --workspace @1024pix/ember-testing-library",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "prepare": "npm run build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "npm start --workspace @1024pix/ember-testing-library -- --no-watch.clearScreen",
    "start:test-app": "npm start --workspace test-app",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-testing-library",
    "test-app"
  ],
  "engines": {
    "node": "12.* || 14.* || >= 16"
  }
}