{
  "name": "@1024pix/ember-testing-library",
  "version": "0.7.1",
  "description": "Package for Testing Library usage in Ember applications.",
  "keywords": [
    "ember-addon",
    "testing",
    "ui",
    "dom",
    "unit",
    "integration",
    "functional",
    "end-to-end",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1024pix/ember-testing-library.git"
  },
  "license": "MIT",
  "author": "Pix",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:watch": "npm-run-all lint test:*",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@testing-library/dom": "~8.20.0",
    "broccoli-funnel": "^3.0.8",
    "ember-auto-import": "^2.6.0",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^3.0.0",
    "@embroider/test-setup": "^3.0.0",
    "@babel/eslint-parser": "^7.11.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.12.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^11.0.0",
    "ember-source": "^5.0.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.18.0",
    "ember-try": "^3.0.0-beta",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "qunit": "^2.19.0",
    "qunit-dom": "^2.0.0",
    "testem": "^3.10.1",
    "webpack": "^5.65.0"
  },
  "overrides": {
    "testem": {
      "fireworm": "^0.7.2"
    }
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/1024pix/ember-testing-library/issues"
  },
  "homepage": "https://github.com/1024pix/ember-testing-library#readme",
  "main": "index.js"
}
